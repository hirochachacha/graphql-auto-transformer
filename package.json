{
  "name": "graphql-auto-transformer",
  "version": "1.1.2",
  "main": "./lib/index.js",
  "author": "Hiroshi Ioka",
  "license": "Apache-2.0",
  "description": "A GraphQL transform that control accessibility of auto generated fields",
  "homepage": "https://github.com/hirochachacha/graphql-auto-transformer#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/hirochachacha/graphql-auto-transformer"
  },
  "keywords": [
    "graphql",
    "amplify",
    "appsync",
    "aws"
  ],
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "lint": "eslint 'src/**/*.ts'",
    "format": "eslint 'src/**/*.ts' --fix",
    "clean": "rm -rf lib"
  },
  "dependencies": {
    "graphql": "^14.5.8",
    "graphql-transformer-common": "^4.12.0",
    "graphql-transformer-core": "^6.12.0"
  },
  "devDependencies": {
    "@types/graphql": "^14.5.0",
    "@types/jest": "^24.9.0",
    "@types/node": "^13.1.7",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "graphql-dynamodb-transformer": "^6.12.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.3.0",
    "typescript": "^3.7.5"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://localhost",
    "testRegex": "(src/__tests__/.*.test.*)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
